{
  "name": "tendermanager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:unit": "vitest run --config vitest.config.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build:production": "bash scripts/build-production.sh",
    "docker:build": "docker build -t tendermanager .",
    "docker:run": "docker run -p 3000:3000 --env-file .env tendermanager"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-time": "^3.0.4",
    "@types/d3-time-format": "^4.0.3",
    "@types/node": "^20.11.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.0",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@rollup/rollup-linux-arm64-gnu": "^4.57.1",
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tiptap/core": "^3.19.0",
    "@tiptap/extension-font-family": "^3.20.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/extension-table": "^3.19.0",
    "@tiptap/extension-table-cell": "^3.19.0",
    "@tiptap/extension-table-header": "^3.19.0",
    "@tiptap/extension-table-row": "^3.19.0",
    "@tiptap/extension-text-style": "^3.20.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "d3-scale": "^4.0.2",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "docx": "^8.5.0",
    "docxtemplater": "^3.68.2",
    "mammoth": "^1.11.0",
    "openai": "^6.18.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.2.2",
    "pizzip": "^3.2.0",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}
